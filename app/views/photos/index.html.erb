<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag "photos" %>
  </head>

  <body>
    <% @photos.each do |photo| %>
    <div class="show-photo">
      <a href=<%="/photos/#{photo.id}"%>>
        <%= image_tag "/photos/#{photo.id}/show_file"%>
      </a>
      <% photo.tags.each do |p| %>
      <%= "#{p.name} " %>
      <% end %>
    </div>
    <% end %>
  </body>
</html>
